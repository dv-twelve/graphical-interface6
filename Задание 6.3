    public partial class Form4 : Form
    {
        public Form4()
        {
            InitializeComponent();
        }

        private void button1_Click(object sender, EventArgs e)
        {
            try
            {
                label3.Visible = false;
                int n = int.Parse(tbN.Text);
                int m = int.Parse(tbM.Text);
                Three o = new Three(n, m);
                labArr.Text = "Массив:\n" + o.Show();
                if (n != m) throw new Exception("Отсутствуют диагонали");
                labRes.Text = "Результат:\n" + o.Diag();
            }
            catch(System.FormatException)
            {
                labRes.Text = "";
                labArr.Text = "";
                label3.Text = "Введены некорректные данные!";
                label3.Visible = true;
            }
            catch(Exception exc)
            {
                labRes.Text = "";
                label3.Text = exc.Message;
                label3.Visible = true;
            }
        }
    }
