        public Form5()
        {
            InitializeComponent();
        }
        int[] f(string str)
        {
            int[] arr = new int[3];
            string[] str2 = str.Split(' ');
            for (int i = 0; i < 3; i++)
            {
                arr[i] = int.Parse(str2[i]);
            }
            return arr;
        }
        private void button1_Click(object sender, EventArgs e)
        {
            try
            {
                labException.Visible = false;
                int N = int.Parse(tbN.Text);
                Four o = new Four(N);
                labArr.Text = "Массив:\n" + o.Show();
                if (N != 3) throw new Exception("Массив должен быть 3 на 3!");
                string vec = tbVec.Text;
                int[] arr = f(vec);
                if (arr.Length != 3) throw new Exception("Вектор должен быть из трех элементов!");
                o.Change(arr);
                labRes.Text = "Результат:\n" + o.Show();
            }
            catch (System.FormatException)
            {
                labRes.Text = "";
                labArr.Text = "";
                labException.Text = "Введены некорректные данные!";
                labException.Visible = true;
            }
            catch (Exception exc)
            {
                labRes.Text = "";
                labException.Text = exc.Message;
                labException.Visible = true;
            }
        }
    }
