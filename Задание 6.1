    public partial class Form2 : Form
    {
        public Form2()
        {
            InitializeComponent();
        }

        private void button1_Click(object sender, EventArgs e)
        {
            try
            {
                One o = new One(int.Parse(tbN.Text), int.Parse(tbN2.Text), int.Parse(tbM2.Text));
                labArr.Text = "Одномерный массив:\n" + o.Show();
                labArr2.Text = "Двумерный массив:\n" + o.Show2();
                o.Replace(int.Parse(tbA.Text), int.Parse(tbB.Text));
                labRes1.Text = "Результат:\n" + o.Show();
                labRes2.Text = "Результат:\n" + o.Show2();
            }
            catch(System.FormatException)
            {
                labException.Text = "Введены некорректные данные!";
                labException.Visible = true;
            }
        }
    }
